{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "types": ["node"],
    "strict": true
  },
  "include": ["vite.config.ts", "scripts"]
}
